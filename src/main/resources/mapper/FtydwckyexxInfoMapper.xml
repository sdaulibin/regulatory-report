<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdjictec.xdfin.regulatory.report.mapper.FtydwckyexxInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sdjictec.xdfin.regulatory.report.entity.FtydwckyexxInfo">
        <result column="sjrq" property="sjrq" />
        <result column="ckzhbm" property="ckzhbm" />
        <result column="ckxh" property="ckxh" />
        <result column="nbjgh" property="nbjgh" />
        <result column="khh" property="khh" />
        <result column="bz" property="bz" />
        <result column="ckye" property="ckye" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sjrq, ckzhbm, ckxh, nbjgh, khh, bz, ckye
    </sql>
    <sql id="concatStr">
        select concat(IFNULL(sjrq,''),'|@|',IFNULL(ckzhbm,''),'|@|',IFNULL(ckxh,''),'|@|',IFNULL(nbjgh,''),'|@|',IFNULL(khh,''),'|@|',IFNULL(bz,''),'|@|',IFNULL(ckye,'')) from ftydwckyexx_info
    </sql>
    <select id="getContactStr" resultType="java.lang.String">
        <include refid="concatStr"/>
        where sjrq=#{sjrq}
    </select>
</mapper>
