<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdjictec.xdfin.regulatory.report.mapper.TyckjcxxInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sdjictec.xdfin.regulatory.report.entity.TyckjcxxInfo">
        <result column="sjrq" property="sjrq" />
        <result column="nbjgh" property="nbjgh" />
        <result column="khh" property="khh" />
        <result column="jrjglxdm" property="jrjglxdm" />
        <result column="ckzhbm" property="ckzhbm" />
        <result column="cfywlx" property="cfywlx" />
        <result column="qsrq" property="qsrq" />
        <result column="dqrq" property="dqrq" />
        <result column="ckqxlx" property="ckqxlx" />
        <result column="djjzlx" property="djjzlx" />
        <result column="lllx" property="lllx" />
        <result column="sjll" property="sjll" />
        <result column="jzll" property="jzll" />
        <result column="llfdpl" property="llfdpl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        sjrq, nbjgh, khh, jrjglxdm, ckzhbm, cfywlx, qsrq, dqrq, ckqxlx, djjzlx, lllx, sjll, jzll, llfdpl
    </sql>
    <sql id="concatStr">
        select concat(IFNULL(sjrq,''),'|@|',IFNULL(nbjgh,''),'|@|',IFNULL(khh,''),'|@|',IFNULL(jrjglxdm,''),'|@|',IFNULL(ckzhbm,''),'|@|',IFNULL(cfywlx,''),'|@|',IFNULL(qsrq,''),'|@|',IFNULL(dqrq,''),'|@|',IFNULL(ckqxlx,''),'|@|',IFNULL(djjzlx,''),'|@|',IFNULL(lllx,''),'|@|',IFNULL(sjll,''),'|@|',IFNULL(jzll,''),'|@|',IFNULL(llfdpl,'')) from tyckjcxx_info
    </sql>
    <select id="getContactStr" resultType="java.lang.String">
        <include refid="concatStr"/>
        where sjrq=#{sjrq}
    </select>
</mapper>
